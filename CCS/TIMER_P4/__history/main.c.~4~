#include <main.h>
#include <lcd.c>
void fun()
{
   if(input(pin_b0) == 0)
   {
      delay_ms(20);
      if(input(pin_b0) == 0)
      {
         //lcd_gotoxy(
      }
   }
}
void main()
{
   lcd_init();
   setup_timer_0(T0_EXT_H_TO_L|T0_DIV_1);
   set_timer0(0);
   while(TRUE)
   {
      lcd_gotoxy(4, 1);
      printf(lcd_putc,"NHAN START");
      lcd_gotoxy(3, 2);
      printf(lcd_putc,"DE KHOI DONG");
   }

}
